---
import Layout from "../layouts/Layout.astro";
// src/pages/impact.astro
import { Image } from 'astro:assets';

const caseStudies = [
    {
        name: "AWRA",
        tagline: "Revolutionizing Public Speaking Through Virtual Reality",
        industry: "Virtual Reality / HealthTech",
        location: "Ethiopia",
        image: "/cs/awra.png",
        link: "https://awra-web.vercel.app",
        description: "Together with AWRA, we pioneered a groundbreaking VR solution to combat glossophobia, helping thousands overcome their fear of public speaking.",
        challenge: "AWRA needed to create an immersive, effective solution for treating glossophobia (fear of public speaking) that would be accessible through the Oculus platform.",
        solution: [
            "Strategic planning and market analysis",
            "VR experience design and development",
            "User experience optimization",
            "Oculus platform integration",
            "Launch strategy execution"
        ],
        impact: [
            "Successfully launched on Oculus Store",
        ],
        quote: "MukadazLabs helped us transform our vision into reality, creating a VR experience that truly changes lives.",
        quoteAuthor: "AWRA Leadership Team"
    },
    {
        name: "Analhorology",
        tagline: "Crafting Digital Excellence for Luxury Timepieces",
        industry: "Luxury Retail",
        location: "Nigeria",
        image: "/cs/annnal.png",
        link: "https://analhorology.com.ng",
        description: "Transforming a traditional luxury watch company into a digital-first brand while maintaining its premium heritage.",
        challenge: "Analhorology needed to transition into the digital age while preserving their luxury brand identity and enhancing their market presence.",
        solution: [
            "Comprehensive digital transformation strategy",
            "Brand repositioning",
            "Digital marketing framework",
            "Customer experience enhancement"
        ],
        impact: [
            "300% increase in online visibility",
            "150% growth in digital sales",
            "40% increase in customer engagement",
            "Successful expansion to international markets"
        ],
        quote: "MukadazLabs understood our vision and helped us maintain our luxury essence while embracing digital innovation.",
        quoteAuthor: "Analhorology Management"
    },
    {
        name: "Yemidyn",
        tagline: "Fashion Meets Digital Innovation",
        industry: "Fashion E-commerce",
        location: "Nigeria",
        image: "/cs/yemmmmm.png",
        link: "https://yemidyn.com",
        description: "Empowering a vibrant fashion brand with a cutting-edge digital platform for seamless shopping experiences.",
        challenge: "Yemidyn needed to transform their traditional fashion business into a competitive e-commerce platform while maintaining their unique brand identity.",
        solution: [
            "E-commerce strategy development",
            "Custom platform development",
            "Integration of advanced features",
            "Mobile-first approach",
            "Digital marketing implementation"
        ],
        impact: [
            "200% increase in online sales",
            "45% improvement in customer retention",
            "60% increase in mobile purchases",
        ],
        quote: "Our digital transformation journey with MukadazLabs exceeded all expectations.",
        quoteAuthor: "Yemidyn Founder"
    },
    {
        name: "Personal Impact",
        tagline: "Digital Transformation in Personal Development",
        industry: "Coaching & Personal Development",
        location: "International",
        image: "/cs/mih.png",
        link: "https://mihaelaprundu.com",
        description: "Elevating a relationship coaching practice through strategic digital transformation and brand positioning.",
        challenge: "Mihaela Prundu needed to establish a strong digital presence and create an engaging platform for her coaching services.",
        solution: [
            "Brand strategy development",
            "Digital presence creation",
            "Content strategy implementation",
            "Website development",
            "Marketing automation setup"
        ],
        impact: [
            "Growth in client base",
            "Global reach expansion",
            "250% increase in engagement",
            
        ],
        quote: "MukadazLabs transformed my vision into a powerful digital presence that resonates with clients worldwide.",
        quoteAuthor: "Mihaela Prundu"
    }
];
---
<Layout title="Impacts">
<div class="bg-white">
    <!-- Hero Section -->
    <div class="relative isolate overflow-hidden bg-gradient-to-b from-indigo-100/20">
        <div class="absolute inset-x-0 -top-40 -z-10 transform-gpu overflow-hidden blur-3xl sm:-top-80">
            <div class="relative left-[calc(50%-11rem)] aspect-[1155/678] w-[36.125rem] -translate-x-1/2 rotate-[30deg] bg-gradient-to-tr from-[#ff80b5] to-[#9089fc] opacity-30 sm:left-[calc(50%-30rem)] sm:w-[72.1875rem]"></div>
        </div>
        
        <div class="mx-auto max-w-7xl px-6 py-24 sm:py-32 lg:px-8">
            <div class="mx-auto max-w-2xl text-center">
                <h2 class="text-4xl font-bold tracking-tight text-gray-900 sm:text-6xl">Impact Stories</h2>
                <p class="mt-6 text-lg leading-8 text-gray-600">
                    Together, we architect digital futures and transform visions into reality. Explore how we've helped businesses across Africa and beyond embrace innovation and achieve extraordinary results.
                </p>
            </div>
        </div>
    </div>

    <!-- Case Studies Section -->
    {caseStudies.map((study, index) => (
        <div class={`relative isolate overflow-hidden ${index % 2 === 0 ? 'bg-white' : 'bg-gray-50'} py-24 sm:py-32`}>
            <div class="mx-auto max-w-7xl px-6 lg:px-8">
                <div class="mx-auto max-w-2xl lg:mx-0 lg:max-w-none">
                    <div class="grid grid-cols-1 gap-x-8 gap-y-16 lg:grid-cols-2">
                        <div class="lg:order-2">
                            <img
                                src={study.image}
                                alt={study.name}
                                class="aspect-[16/9] w-full rounded-2xl object-cover shadow-2xl"
                            />
                        </div>
                        <div class="lg:order-1">
                            <div class="flex items-center gap-x-4 text-xs">
                                <span class="text-gray-500">{study.industry}</span>
                                <span class="text-gray-500">{study.location}</span>
                            </div>
                            <h3 class="mt-4 text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">
                                {study.name}
                            </h3>
                            <p class="mt-2 text-lg font-semibold text-indigo-600">{study.tagline}</p>
                            <p class="mt-6 text-lg leading-8 text-gray-600">
                                {study.description}
                            </p>
                            
                            <div class="mt-10">
                                <h4 class="text-lg font-semibold text-gray-900">The Challenge</h4>
                                <p class="mt-4 text-gray-600">{study.challenge}</p>
                            </div>

                            <div class="mt-10">
                                <h4 class="text-lg font-semibold text-gray-900">Our Solution</h4>
                                <ul class="mt-4 space-y-3">
                                    {study.solution.map(item => (
                                        <li class="flex gap-x-3">
                                            <svg class="h-6 w-5 flex-none text-indigo-600" viewBox="0 0 20 20" fill="currentColor">
                                                <path fill-rule="evenodd" d="M16.704 4.153a.75.75 0 01.143 1.052l-8 10.5a.75.75 0 01-1.127.075l-4.5-4.5a.75.75 0 011.06-1.06l3.894 3.893 7.48-9.817a.75.75 0 011.05-.143z" clip-rule="evenodd" />
                                            </svg>
                                            <span class="text-gray-600">{item}</span>
                                        </li>
                                    ))}
                                </ul>
                            </div>

                            <div class="mt-10">
                                <h4 class="text-lg font-semibold text-gray-900">Impact Created</h4>
                                <ul class="mt-4 space-y-3">
                                    {study.impact.map(item => (
                                        <li class="flex gap-x-3">
                                            <svg class="h-6 w-5 flex-none text-green-600" viewBox="0 0 20 20" fill="currentColor">
                                                <path fill-rule="evenodd" d="M10 17a7 7 0 100-14 7 7 0 000 14zm0 1a8 8 0 100-16 8 8 0 000 16z M10 6a.75.75 0 01.75.75v3.5a.75.75 0 01-1.5 0v-3.5A.75.75 0 0110 6zm0 9a1 1 0 100-2 1 1 0 000 2z" clip-rule="evenodd" />
                                            </svg>
                                            <span class="text-gray-600">{item}</span>
                                        </li>
                                    ))}
                                </ul>
                            </div>

                            <figure class="mt-10">
                                <blockquote class="text-lg font-semibold leading-8 text-gray-900">
                                    <p>"{study.quote}"</p>
                                </blockquote>
                                <figcaption class="mt-3 text-sm leading-6 text-gray-600">{study.quoteAuthor}</figcaption>
                            </figure>

                            {study.link && (
                                <div class="mt-10">
                                    <a
                                        href={study.link}
                                        target="_blank"
                                        rel="noopener"
                                        class="inline-flex items-center gap-x-2 rounded-full bg-indigo-600 px-4 py-2.5 text-sm font-semibold text-white shadow-sm hover:bg-indigo-500 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-indigo-600"
                                    >
                                        Visit Website
                                        <svg class="h-5 w-5" viewBox="0 0 20 20" fill="currentColor">
                                            <path fill-rule="evenodd" d="M5.22 14.78a.75.75 0 001.06 0l7.22-7.22v5.69a.75.75 0 001.5 0v-7.5a.75.75 0 00-.75-.75h-7.5a.75.75 0 000 1.5h5.69l-7.22 7.22a.75.75 0 000 1.06z" clip-rule="evenodd" />
                                        </svg>
                                    </a>
                                </div>
                            )}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    ))}

    <!-- CTA Section -->
    <div class="bg-white">
        <div class="mx-auto max-w-7xl py-24 sm:px-6 sm:py-32 lg:px-8">
            <div class="relative isolate overflow-hidden bg-gray-900 px-6 py-24 text-center shadow-2xl sm:rounded-3xl sm:px-16">
                <h2 class="mx-auto max-w-2xl text-3xl font-bold tracking-tight text-white sm:text-4xl">
                    Ready to Transform Your Vision Into Reality?
                </h2>
                <p class="mx-auto mt-6 max-w-xl text-lg leading-8 text-gray-300">
                    Let's architect your digital future together. Our team is ready to help you achieve extraordinary results.
                </p>
                <div class="mt-10 flex items-center justify-center gap-x-6">
                    <a
                        href="/contact"
                        class="rounded-md bg-white px-3.5 py-2.5 text-sm font-semibold text-gray-900 shadow-sm hover:bg-gray-100 focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-white"
                    >
                        Get Started
                    </a>
    
                </div>
            </div>
        </div>
    </div>
</div>
</Layout>